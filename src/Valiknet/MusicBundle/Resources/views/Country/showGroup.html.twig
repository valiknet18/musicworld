{% extends 'ValiknetMusicBundle:Country:country_webber.html.twig' %}

{% trans_default_domain "country" %}

{% block title %}
    {{ "countries.title.groupsCountry"|trans({"%countryName%" : country.name}) }}
{% endblock %}

{% block links %}
    <a href="{{ path('valiknet_group_list') }}">{{ "countries.countries"|trans() }}</a>>{{ country.name }}><a href="{{ path('valiknet_country_group_view', {"slug" : country.slug}) }}">{{ "countries.navigation.groups"|trans() }}</a>
{% endblock %}

{% block country_content %}
    <div class="col-lg-12">
        {% include "@ValiknetMusic/Group/blocks/listGroups.html.twig" with {"groups" : groups} %}
    </div>
{% endblock %}
